		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ВложенныйЗапрос.Company КАК Co,
	ВложенныйЗапрос.CustomerNumber КАК ClientID,
	ВложенныйЗапрос.CustomerName,
	ВложенныйЗапрос.AccountingPeriod КАК AccountingPeriod,
	ВложенныйЗапрос.FiscalYear КАК Year,
	ВложенныйЗапрос.ArInvoice КАК ArInvoice,
	ВложенныйЗапрос.AccountUnit,
	СУММА(ВложенныйЗапрос.TranAmount) КАК TranVAT,
	ВложенныйЗапрос.CurrencyCode КАК Curr,
	СУММА(ВложенныйЗапрос.BaseAmount) КАК BaseVAT,
	ВложенныйЗапрос.РегистраторДокументПодразделениеОрганизации КАК FiscalLocation,
	ВложенныйЗапрос.РегистраторДокументНомер КАК Akt,
	ВложенныйЗапрос.РегистраторДокументДата КАК Akt_Date,
	ВложенныйЗапрос1.Номер КАК F_Invoice,
	ВложенныйЗапрос1.Дата КАК F_Invoice_Date,
	ВложенныйЗапрос.РегистраторДокументДатаПроведения КАК PostingDate,
	ВложенныйЗапрос.РегистраторДокументНалоговыйПериод КАК TaxPeriod
ИЗ
	(ВЫБРАТЬ
		ПроводкиDSS_SB.Company КАК Company,
		ПроводкиDSS_SB.CustomerNumber КАК CustomerNumber,
		ПроводкиDSS_SB.CustomerName КАК CustomerName,
		ПроводкиDSS_SB.AccountingPeriod КАК AccountingPeriod,
		ПроводкиDSS_SB.FiscalYear КАК FiscalYear,
		ПроводкиDSS_SB.ArInvoice.ArInvoice КАК ArInvoice,
		ПроводкиDSS_SB.AccountUnit КАК AccountUnit,
		ПроводкиDSS_SB.TranAmount КАК TranAmount,
		ПроводкиDSS_SB.CurrencyCode КАК CurrencyCode,
		ПроводкиDSS_SB.BaseAmount КАК BaseAmount,
		ПроводкиDSS_SB.ArInvoice.Документ.ПодразделениеОрганизации КАК РегистраторДокументПодразделениеОрганизации,
		ПроводкиDSS_SB.ArInvoice.Документ.Номер КАК РегистраторДокументНомер,
		ПроводкиDSS_SB.ArInvoice.Документ.Дата КАК РегистраторДокументДата,
		ПроводкиDSS_SB.ArInvoice.Документ.ДатаПроведения КАК РегистраторДокументДатаПроведения,
		ПроводкиDSS_SB.ArInvoice.Документ.НалоговыйПериод КАК РегистраторДокументНалоговыйПериод,
		ПроводкиDSS_SB.ArInvoice.Документ КАК РегистраторДокумент
	ИЗ
		РегистрНакопления.ПроводкиDSS_SB КАК ПроводкиDSS_SB
	ГДЕ
		ПроводкиDSS_SB.Type = &amp;Тип
		И НАЧАЛОПЕРИОДА(ПроводкиDSS_SB.FiscalYear, ГОД) = &amp;Год
		И ПроводкиDSS_SB.AccountingPeriod МЕЖДУ &amp;Нач И &amp;Кон) КАК ВложенныйЗапрос
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ РАЗЛИЧНЫЕ
			СчетФактураВыданный.Номер КАК Номер,
			СчетФактураВыданный.Дата КАК Дата,
			СчетФактураВыданный.ДокументОснование КАК ДокументОснование
		ИЗ
			Документ.СчетФактураВыданный КАК СчетФактураВыданный
		ГДЕ
			СчетФактураВыданный.Проведен) КАК ВложенныйЗапрос1
		ПО ВложенныйЗапрос.РегистраторДокумент = ВложенныйЗапрос1.ДокументОснование

СГРУППИРОВАТЬ ПО
	ВложенныйЗапрос.Company,
	ВложенныйЗапрос.CustomerNumber,
	ВложенныйЗапрос.CustomerName,
	ВложенныйЗапрос.AccountingPeriod,
	ВложенныйЗапрос.FiscalYear,
	ВложенныйЗапрос.ArInvoice,
	ВложенныйЗапрос.AccountUnit,
	ВложенныйЗапрос.CurrencyCode,
	ВложенныйЗапрос.РегистраторДокументПодразделениеОрганизации,
	ВложенныйЗапрос.РегистраторДокументНомер,
	ВложенныйЗапрос.РегистраторДокументДата,
	ВложенныйЗапрос.РегистраторДокументДатаПроведения,
	ВложенныйЗапрос.РегистраторДокументНалоговыйПериод,
	ВложенныйЗапрос1.Номер,
	ВложенныйЗапрос1.Дата

УПОРЯДОЧИТЬ ПО
	Co УБЫВ,
	Year,
	AccountingPeriod,
	ClientID,
	ArInvoice</query>
