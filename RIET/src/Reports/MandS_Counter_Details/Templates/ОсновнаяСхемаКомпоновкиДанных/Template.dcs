ПОМЕСТИТЬ ДанныеБюджетов
ИЗ
	РегистрСведений.rgsBudgetSums КАК rgsBudgetSums
ГДЕ
	rgsBudgetSums.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И rgsBudgetSums.BudgetType = &amp;BudgetType
	И rgsBudgetSums.TypeInternationalDomestic = ЗНАЧЕНИЕ(Перечисление.DomesticInternational.International)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	СтрокиИнвойса.Ссылка,
	СтрокиИнвойса.КостЦентр.Geomarket,
	СтрокиИнвойса.КостЦентр.SubGeomarket,
	СтрокиИнвойса.КостЦентр.Segment,
	СтрокиИнвойса.КостЦентр.SubSegment,
	0,
	СтрокиИнвойса.Сумма,
	0,
	СтрокиИнвойса.Currency,
	ВЫБОР
		КОГДА DOCsInvoices.Ссылка.Requested &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА НАЧАЛОПЕРИОДА(DOCsInvoices.Ссылка.Booked, МЕСЯЦ)
		ИНАЧЕ НАЧАЛОПЕРИОДА(DOCsInvoices.Ссылка.Granted, МЕСЯЦ)
	КОНЕЦ,
	DOCsInvoices.Ссылка.Номер,
	DOCsInvoices.Ссылка.Дата,
	DOCsInvoices.Ссылка.GOLD,
	DOCsInvoices.Ссылка.POD,
	DOCsInvoices.Ссылка.MOT,
	DOCsInvoices.Ссылка.RequestedPOA,
	DOCsInvoices.Ссылка.Urgency,
	DOCsInvoices.Ссылка.Granted,
	DOCsInvoices.Ссылка.CurrentStatus,
	DOCsInvoices.Ссылка.CurrentComment,
	DOCsInvoices.Ссылка.GL_FromSegment,
	СтрокиИнвойса.СтрокаЗаявкиНаЗакупку,
	СтрокиИнвойса.КодПоИнвойсу,
	СтрокиИнвойса.Сумма,
	СтрокиИнвойса.Цена,
	СтрокиИнвойса.Количество,
	ВЫРАЗИТЬ(СтрокиИнвойса.НаименованиеТовара КАК СТРОКА(1024)),
	СтрокиИнвойса.Final,
	СтрокиИнвойса.СтрокаЗаявкиНаЗакупку.КодПоставщика,
	СтрокиИнвойса.СтрокаЗаявкиНаЗакупку.Владелец.Комментарий,
	СтрокиИнвойса.Классификатор,
	СтрокиИнвойса.НомерЗаявкиНаЗакупку
ИЗ
	Справочник.СтрокиИнвойса КАК СтрокиИнвойса
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.КонсолидированныйПакетЗаявокНаПеревозку.Инвойсы КАК DOCsInvoices
		ПО СтрокиИнвойса.Инвойс = DOCsInvoices.Инвойс
ГДЕ
	СтрокиИнвойса.Классификатор = ЗНАЧЕНИЕ(Перечисление.ТипыЗаказа.E)
	И НЕ СтрокиИнвойса.ПометкаУдаления
	И НЕ DOCsInvoices.Ссылка.Отменен
	И DOCsInvoices.Ссылка.CurrentStage = ЗНАЧЕНИЕ(Перечисление.DOCStages.Booked)
	И DOCsInvoices.Ссылка.Booked МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И DOCsInvoices.Ссылка.Дата &gt;= ДАТАВРЕМЯ(2017, 1, 1, 0, 0, 0)
	И НЕ DOCsInvoices.Ссылка.ПометкаУдаления
	И НЕ DOCsInvoices.Ссылка.HouseKeeping

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	СтрокиИнвойса.Ссылка,
	СтрокиИнвойса.КостЦентр.Geomarket,
	СтрокиИнвойса.КостЦентр.SubGeomarket,
	СтрокиИнвойса.КостЦентр.Segment,
	СтрокиИнвойса.КостЦентр.SubSegment,
	СтрокиИнвойса.Сумма,
	0,
	0,
	СтрокиИнвойса.Currency,
	&amp;НачалоПериода,
	DOCsInvoices.Ссылка.Номер,
	DOCsInvoices.Ссылка.Дата,
	DOCsInvoices.Ссылка.GOLD,
	DOCsInvoices.Ссылка.POD,
	DOCsInvoices.Ссылка.MOT,
	DOCsInvoices.Ссылка.RequestedPOA,
	DOCsInvoices.Ссылка.Urgency,
	DOCsInvoices.Ссылка.Granted,
	DOCsInvoices.Ссылка.CurrentStatus,
	DOCsInvoices.Ссылка.CurrentComment,
	DOCsInvoices.Ссылка.GL_FromSegment,
	СтрокиИнвойса.СтрокаЗаявкиНаЗакупку,
	СтрокиИнвойса.КодПоИнвойсу,
	СтрокиИнвойса.Сумма,
	СтрокиИнвойса.Цена,
	СтрокиИнвойса.Количество,
	ВЫРАЗИТЬ(СтрокиИнвойса.НаименованиеТовара КАК СТРОКА(1024)),
	СтрокиИнвойса.Final,
	СтрокиИнвойса.СтрокаЗаявкиНаЗакупку.КодПоставщика,
	СтрокиИнвойса.СтрокаЗаявкиНаЗакупку.Владелец.Комментарий,
	СтрокиИнвойса.Классификатор,
	СтрокиИнвойса.НомерЗаявкиНаЗакупку
ИЗ
	Справочник.СтрокиИнвойса КАК СтрокиИнвойса
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.КонсолидированныйПакетЗаявокНаПеревозку.Инвойсы КАК DOCsInvoices
		ПО СтрокиИнвойса.Инвойс = DOCsInvoices.Инвойс
ГДЕ
	СтрокиИнвойса.Классификатор = ЗНАЧЕНИЕ(Перечисление.ТипыЗаказа.E)
	И НЕ СтрокиИнвойса.ПометкаУдаления
	И НЕ DOCsInvoices.Ссылка.Отменен
	И ВЫБОР
			КОГДА DOCsInvoices.Ссылка.Requested &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА DOCsInvoices.Ссылка.Booked = ДАТАВРЕМЯ(1, 1, 1)
			ИНАЧЕ DOCsInvoices.Ссылка.Granted = ДАТАВРЕМЯ(1, 1, 1)
		КОНЕЦ
	И DOCsInvoices.Ссылка.CurrentStage &lt;&gt; ЗНАЧЕНИЕ(Перечисление.DOCStages.Booked)
	И DOCsInvoices.Ссылка.GL_FromSegment
	И НЕ DOCsInvoices.Ссылка.ПометкаУдаления
	И НЕ DOCsInvoices.Ссылка.HouseKeeping
	И DOCsInvoices.Ссылка.Дата &gt;= ДАТАВРЕМЯ(2017, 1, 1, 0, 0, 0)
;

