		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	РеализацияТоваровУслугТовары.Ссылка.Номер,
	РеализацияТоваровУслугТовары.Ссылка.Дата,
	РеализацияТоваровУслугТовары.Ссылка.Контрагент.LawsonID КАК КодКонтрагента,
	ВЫРАЗИТЬ(РеализацияТоваровУслугТовары.Ссылка.Контрагент.КонтрагентLawson.Наименование КАК СТРОКА(250)) КАК НаименованиеКонтрагентаАнгл,
	ВЫРАЗИТЬ(РеализацияТоваровУслугТовары.Ссылка.Контрагент.НаименованиеПолное КАК СТРОКА(250)) КАК НаименованиеКонтрагента,
	РеализацияТоваровУслугТовары.Ссылка.Контрагент.ИНН + " / " + РеализацияТоваровУслугТовары.Ссылка.Контрагент.КПП КАК ИНН_КПП,
	РеализацияТоваровУслугТовары.Ссылка.ВалютаДокумента КАК Валюта,
	ВЫБОР
		КОГДА РеализацияТоваровУслугТовары.Ссылка.СуммаВключаетНДС
			ТОГДА РеализацияТоваровУслугТовары.Сумма - РеализацияТоваровУслугТовары.СуммаНДС
		ИНАЧЕ РеализацияТоваровУслугТовары.Сумма
	КОНЕЦ КАК СуммаБезНДС,
	РеализацияТоваровУслугТовары.Ссылка.КурсВзаиморасчетов КАК Курс,
	РеализацияТоваровУслугТовары.КостЦентр КАК AccountingUnit,
	РеализацияТоваровУслугТовары.СуммаНДС,
	ВЫБОР
		КОГДА РеализацияТоваровУслугТовары.Ссылка.СуммаВключаетНДС
			ТОГДА РеализацияТоваровУслугТовары.Сумма
		ИНАЧЕ РеализацияТоваровУслугТовары.Сумма + РеализацияТоваровУслугТовары.СуммаНДС
	КОНЕЦ КАК СуммаСНДС,
	РеализацияТоваровУслугТовары.Ссылка КАК Ссылка,
	НАЧАЛОПЕРИОДА(РеализацияТоваровУслугТовары.Ссылка.ДатаПроведения, ДЕНЬ) КАК ДатаПроведения,
	РеализацияТоваровУслугТовары.Ссылка.Контрагент
ПОМЕСТИТЬ ТЧРеализаций
ИЗ
	Документ.РеализацияТоваровУслуг.Товары КАК РеализацияТоваровУслугТовары
ГДЕ
	РеализацияТоваровУслугТовары.Ссылка.НалоговыйПериод МЕЖДУ &amp;НачалоПериода И КОНЕЦПЕРИОДА(&amp;КонецПериода, ДЕНЬ)
	И РеализацияТоваровУслугТовары.Ссылка.Проведен
	И РеализацияТоваровУслугТовары.КостЦентр.Segment = &amp;ALS
	И РеализацияТоваровУслугТовары.Ссылка.СуммаДокумента &gt; 0
	И РеализацияТоваровУслугТовары.Ссылка.ПодразделениеОрганизации = &amp;НалоговаяРегистрация
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КурсыВалют.Валюта,
	КурсыВалют.Курс,
	КурсыВалют.Период
ПОМЕСТИТЬ КурсыВалютSLB
ИЗ
	РегистрСведений.КурсыВалют КАК КурсыВалют
ГДЕ
	КурсыВалют.Валюта = &amp;SLB
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТЧРеализаций.Ссылка,
	ТЧРеализаций.Номер,
	ТЧРеализаций.Дата,
	ТЧРеализаций.КодКонтрагента,
	ТЧРеализаций.НаименованиеКонтрагентаАнгл,
	ТЧРеализаций.НаименованиеКонтрагента,
	ТЧРеализаций.ИНН_КПП,
	ТЧРеализаций.Валюта,
	СУММА(ТЧРеализаций.СуммаБезНДС) КАК СуммаБезНДС,
	ВЫБОР
		КОГДА ТЧРеализаций.Валюта = &amp;Руб
			ТОГДА КурсыВалютSLB.Курс
		ИНАЧЕ ТЧРеализаций.Курс
	КОНЕЦ КАК Курс,
	МАКСИМУМ(ТЧРеализаций.AccountingUnit) КАК AccountingUnit,
	СУММА(ТЧРеализаций.СуммаНДС) КАК СуммаНДС,
	СУММА(ТЧРеализаций.СуммаСНДС) КАК СуммаСНДС,
	ТЧРеализаций.ДатаПроведения,
	ТЧРеализаций.Контрагент
ПОМЕСТИТЬ Реализации
ИЗ
	ТЧРеализаций КАК ТЧРеализаций
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалютSLB КАК КурсыВалютSLB
		ПО (НАЧАЛОПЕРИОДА(ТЧРеализаций.ДатаПроведения, ДЕНЬ) = КурсыВалютSLB.Период)

СГРУППИРОВАТЬ ПО
	ТЧРеализаций.Дата,
	ТЧРеализаций.КодКонтрагента,
	ТЧРеализаций.НаименованиеКонтрагентаАнгл,
	ТЧРеализаций.НаименованиеКонтрагента,
	ТЧРеализаций.ИНН_КПП,
	ТЧРеализаций.Валюта,
	ТЧРеализаций.Ссылка,
	ТЧРеализаций.Номер,
	ТЧРеализаций.ДатаПроведения,
	ТЧРеализаций.Контрагент,
	ВЫБОР
		КОГДА ТЧРеализаций.Валюта = &amp;Руб
			ТОГДА КурсыВалютSLB.Курс
		ИНАЧЕ ТЧРеализаций.Курс
	КОНЕЦ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Реализации.Ссылка,
	Реализации.Номер,
	Реализации.Дата,
	Реализации.КодКонтрагента,
	Реализации.НаименованиеКонтрагентаАнгл,
	Реализации.НаименованиеКонтрагента,
	Реализации.ИНН_КПП,
	Реализации.Валюта,
	ВЫБОР
		КОГДА Реализации.Валюта = &amp;Руб
			ТОГДА ВЫРАЗИТЬ(Реализации.СуммаБезНДС / ЕСТЬNULL(Реализации.Курс, 1) КАК ЧИСЛО(15, 2))
		ИНАЧЕ Реализации.СуммаБезНДС
	КОНЕЦ КАК СуммаБезНДС,
	Реализации.AccountingUnit,
	ВЫБОР
		КОГДА Реализации.Валюта = &amp;Руб
			ТОГДА ВЫРАЗИТЬ(Реализации.СуммаНДС / ЕСТЬNULL(Реализации.Курс, 1) КАК ЧИСЛО(15, 2))
		ИНАЧЕ Реализации.СуммаНДС
	КОНЕЦ КАК СуммаНДС,
	ВЫБОР
		КОГДА Реализации.Валюта = &amp;Руб
			ТОГДА ВЫРАЗИТЬ(Реализации.СуммаСНДС / ЕСТЬNULL(Реализации.Курс, 1) КАК ЧИСЛО(15, 2))
		ИНАЧЕ Реализации.СуммаСНДС
	КОНЕЦ КАК СуммаСНДС,
	ВЫБОР
		КОГДА Реализации.Валюта = &amp;Руб
			ТОГДА Реализации.СуммаБезНДС
		ИНАЧЕ ВЫРАЗИТЬ(Реализации.СуммаБезНДС * ЕСТЬNULL(Реализации.Курс, 1) КАК ЧИСЛО(15, 2))
	КОНЕЦ КАК СуммаБезНДСРуб,
	ВЫБОР
		КОГДА Реализации.Валюта = &amp;Руб
			ТОГДА Реализации.СуммаНДС
		ИНАЧЕ ВЫРАЗИТЬ(Реализации.СуммаНДС * ЕСТЬNULL(Реализации.Курс, 1) КАК ЧИСЛО(15, 2))
	КОНЕЦ КАК СуммаНДСРуб,
	ВЫБОР
		КОГДА Реализации.Валюта = &amp;Руб
			ТОГДА Реализации.СуммаСНДС
		ИНАЧЕ ВЫРАЗИТЬ(Реализации.СуммаСНДС * ЕСТЬNULL(Реализации.Курс, 1) КАК ЧИСЛО(15, 2))
	КОНЕЦ КАК СуммаСНДСРуб,
	Реализации.ДатаПроведения,
	Реализации.Курс,
	Реализации.Контрагент,
