ИЗ
	Контрагенты_ДатаКПП КАК Контрагенты_ДатаКПП
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ИсторияКПП КАК ИсторияКПП
		ПО Контрагенты_ДатаКПП.Период = ИсторияКПП.Период
			И Контрагенты_ДатаКПП.Контрагент = ИсторияКПП.Контрагент

ИНДЕКСИРОВАТЬ ПО
	Контрагент
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	табКонтрагентыLawsonID.LawsonID КАК LawsonID,
	табКонтрагентыLawsonID.Контрагент КАК ClientName,
	табПродажи.НалоговыйПериод КАК TaxPeriod,
	табПродажи.РегистраторНомер КАК Act,
	табПродажи.РегистраторДата КАК Act_Date,
	СФ.Номер КАК F_Invoice,
	СФ.Дата КАК F_Invoice_Date,
	табПродажи.Валюта КАК Currency,
	ВЫБОР
		КОГДА табПродажи.Валюта.Код &lt;&gt; "643"
			ТОГДА табПродажи.Курс
		ИНАЧЕ 0
	КОНЕЦ КАК ExchRate,
	ВЫБОР
		КОГДА табПродажи.СуммаНДС &lt;&gt; 0
			ТОГДА "18%"
		ИНАЧЕ "Без НДС"
	КОНЕЦ КАК VatRate,
	ВЫБОР
		КОГДА табПродажи.Валюта.Код = "643"
			ТОГДА табПродажи.СуммаБезНДСРуб
		ИНАЧЕ табПродажи.СуммаБезНДС
	КОНЕЦ КАК D_Net,
	ВЫБОР
		КОГДА табПродажи.Валюта.Код = "643"
			ТОГДА табПродажи.СуммаНДСРуб
		ИНАЧЕ табПродажи.СуммаНДС
	КОНЕЦ КАК D_VAT,
	ВЫБОР
		КОГДА табПродажи.Валюта.Код = "643"
			ТОГДА табПродажи.СуммаБезНДСРуб + табПродажи.СуммаНДСРуб
		ИНАЧЕ табПродажи.СуммаБезНДС + табПродажи.СуммаНДС
	КОНЕЦ КАК D_Gross,
	табПродажи.СуммаБезНДСРуб КАК F_Net,
	табПродажи.СуммаНДСРуб КАК F_VAT,
	табПродажи.СуммаБезНДСРуб + табПродажи.СуммаНДСРуб КАК F_Gross,
	табПродажи.Подразделение КАК Location,
	табПродажи.AU КАК AU,
	табПродажи.AU.Сегмент.Родитель.Родитель КАК Seg,
	табПродажи.Тип КАК Type,
	табПродажи.Номенклатура КАК Description,
	табПродажи.РегистраторОтветственный КАК Operator,
	табПродажи.Подразделение.Наименование КАК ПодразделениеОрганизацииНаименование,
	табПродажи.Период КАК Posted,
	табПродажи.Договор КАК Contract,
	табПродажи.InvoicingCenter,
	табКонтрагентыLawsonID.Контрагент.ИНН КАК ClientИНН,
	ВЫБОР
		КОГДА Контрагенты_КППизИстории.КПП ЕСТЬ NULL 
			ТОГДА табКонтрагентыLawsonID.Контрагент.КПП
		ИНАЧЕ Контрагенты_КППизИстории.КПП
	КОНЕЦ КАК ClientКПП,
	ВЫБОР
		КОГДА табПродажи.Номенклатура ССЫЛКА Справочник.Номенклатура
			ТОГДА ВЫБОР
					КОГДА ЕСТЬNULL(табПродажи.Номенклатура.Catalog.Код, "") &lt;&gt; ""
						ТОГДА ЕСТЬNULL(табПродажи.Номенклатура.Catalog.Код, "")
					ИНАЧЕ ЕСТЬNULL(табПродажи.Номенклатура.Артикул, "")
				КОНЕЦ
		ИНАЧЕ ЕСТЬNULL(табПродажи.Номенклатура.СерийныйНомер, "")
	КОНЕЦ КАК PartNumber,
	табПродажи.НоменклатураНаименование
ИЗ
	табПродажи КАК табПродажи
		ЛЕВОЕ СОЕДИНЕНИЕ СФ КАК СФ
		ПО табПродажи.Регистратор = СФ.ДокументОснование
		ЛЕВОЕ СОЕДИНЕНИЕ табКонтрагентыLawsonID КАК табКонтрагентыLawsonID
		ПО табПродажи.Контрагент = табКонтрагентыLawsonID.Контрагент
		ЛЕВОЕ СОЕДИНЕНИЕ Контрагенты_КППизИстории КАК Контрагенты_КППизИстории
		ПО табПродажи.Контрагент = Контрагенты_КППизИстории.Контрагент

СГРУППИРОВАТЬ ПО
	табПродажи.AU,
	табПродажи.НалоговыйПериод,
	табПродажи.Подразделение,
	табПродажи.Валюта,
	табКонтрагентыLawsonID.LawsonID,
	табПродажи.Тип,
	табПродажи.Период,
	табКонтрагентыLawsonID.Контрагент,
	СФ.Дата,
	СФ.Номер,
	табПродажи.СуммаНДСРуб,
	табПродажи.AU.Сегмент.Родитель.Родитель,
	табПродажи.Подразделение.Наименование,
	табПродажи.Номенклатура,
	табПродажи.СуммаБезНДСРуб,
	табПродажи.РегистраторНомер,
	табПродажи.РегистраторДата,
	табПродажи.РегистраторОтветственный,
	табПродажи.Договор,
	табПродажи.InvoicingCenter,
	ВЫБОР
		КОГДА табПродажи.СуммаНДС &lt;&gt; 0
			ТОГДА "18%"
		ИНАЧЕ "Без НДС"
	КОНЕЦ,
	табКонтрагентыLawsonID.Контрагент.ИНН,
	ВЫБОР
		КОГДА табПродажи.Номенклатура ССЫЛКА Справочник.Номенклатура
			ТОГДА ВЫБОР
					КОГДА ЕСТЬNULL(табПродажи.Номенклатура.Catalog.Код, "") &lt;&gt; ""
						ТОГДА ЕСТЬNULL(табПродажи.Номенклатура.Catalog.Код, "")
					ИНАЧЕ ЕСТЬNULL(табПродажи.Номенклатура.Артикул, "")
				КОНЕЦ
		ИНАЧЕ ЕСТЬNULL(табПродажи.Номенклатура.СерийныйНомер, "")
	КОНЕЦ,
	ВЫБОР
		КОГДА табПродажи.Валюта.Код = "643"
			ТОГДА табПродажи.СуммаБезНДСРуб
		ИНАЧЕ табПродажи.СуммаБезНДС
	КОНЕЦ,
	ВЫБОР
		КОГДА табПродажи.Валюта.Код &lt;&gt; "643"
			ТОГДА табПродажи.Курс
		ИНАЧЕ 0
	КОНЕЦ,
	ВЫБОР
		КОГДА табПродажи.Валюта.Код = "643"
			ТОГДА табПродажи.СуммаНДСРуб
		ИНАЧЕ табПродажи.СуммаНДС
	КОНЕЦ,
	ВЫБОР
		КОГДА табПродажи.Валюта.Код = "643"
			ТОГДА табПродажи.СуммаБезНДСРуб + табПродажи.СуммаНДСРуб
		ИНАЧЕ табПродажи.СуммаБезНДС + табПродажи.СуммаНДС
	КОНЕЦ,
	ВЫБОР
		КОГДА Контрагенты_КППизИстории.КПП ЕСТЬ NULL 
			ТОГДА табКонтрагентыLawsonID.Контрагент.КПП
		ИНАЧЕ Контрагенты_КППизИстории.КПП
	КОНЕЦ,
	табПродажи.НоменклатураНаименование

УПОРЯДОЧИТЬ ПО
	Location,
	LawsonID,
	AU</query>
