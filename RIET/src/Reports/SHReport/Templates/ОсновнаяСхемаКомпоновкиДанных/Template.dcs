		ИНАЧЕ ВЫБОР
				КОГДА TripsNonLawsonApproval.Status = ЗНАЧЕНИЕ(Перечисление.StatusesOfApproval.SentForApproval)
					ТОГДА "LEVEL_1_REQUIRED"
				КОГДА TripsNonLawsonApproval.Status = ЗНАЧЕНИЕ(Перечисление.StatusesOfApproval.Approved)
					ТОГДА "APPROVED BY RLC MANAGER"
				КОГДА TripsNonLawsonApproval.Status = ЗНАЧЕНИЕ(Перечисление.StatusesOfApproval.Rejected)
					ТОГДА "REJECTED BY RLC MANAGER"
				ИНАЧЕ "NOT REQUIRED"
			КОНЕЦ
	КОНЕЦ,
	TripNonLawsonCompaniesStops_Dest.Location,
	StagesOfTripsNonLawsonCompanies.Trip.Дата,
	TripNonLawsonCompaniesStops_Source.Location.FacilityType.Код + "_" + TripNonLawsonCompaniesStops_Dest.Location.FacilityType.Код,
	TripNonLawsonCompaniesStops_Source.Location.Наименование + "_" + TripNonLawsonCompaniesStops_Dest.Location.Наименование,
	TripNonLawsonCompaniesStops_Source.Location.RegionProvince.Код + "_" + TripNonLawsonCompaniesStops_Dest.Location.RegionProvince.Код,
	StagesOfTripsNonLawsonCompanies.Trip.ServiceProvider,
	StagesOfTripsNonLawsonCompanies.Trip.Specialist,
	ВЫБОР
		КОГДА StagesOfTripsNonLawsonCompanies.Stage = ЗНАЧЕНИЕ(Перечисление.TripNonLawsonCompaniesStages.Draft)
				ИЛИ StagesOfTripsNonLawsonCompanies.Stage = ЗНАЧЕНИЕ(Перечисление.TripNonLawsonCompaniesStages.Saved_AwaitingServiceProvidersCosts)
			ТОГДА "ENROUTE_NOT STARTED"
		ИНАЧЕ ВЫБОР
				КОГДА ЕСТЬNULL(TripNonLawsonCompaniesStops_Dest.OnTime, ЗНАЧЕНИЕ(Перечисление.YesNo.Yes)) = ЗНАЧЕНИЕ(Перечисление.YesNo.No)
					ТОГДА "ENROUTE_DELAYED"
				ИНАЧЕ ВЫБОР
						КОГДА ЕСТЬNULL(TripNonLawsonCompaniesStops_Dest.ActualArrivalLocalTime, ДАТАВРЕМЯ(1, 1, 1)) = ДАТАВРЕМЯ(1, 1, 1)
							ТОГДА "ENROUTE_ENROUTE"
						ИНАЧЕ "ENROUTE_COMPLETED"
					КОНЕЦ
			КОНЕЦ
	КОНЕЦ,
	StagesOfTripsNonLawsonCompanies.Trip.Equipment.Код,
	TripNonLawsonCompaniesStops_Source.PlannedDepartureLocalTime,
