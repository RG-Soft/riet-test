		КОГДА ЕСТЬNULL(TripNonLawsonCompaniesParcels.Ссылка.VerifiedByBillingSpecialist, ДАТАВРЕМЯ(1, 1, 1)) = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА "OPENED"
		ИНАЧЕ "CLOSED"
	КОНЕЦ,
	ВЫБОР
		КОГДА TripsNonLawsonApproval.ApprovalLevel = ЗНАЧЕНИЕ(Перечисление.ApprovalLevels.Level1)
			ТОГДА "NOT_STARTED"
		ИНАЧЕ ВЫБОР
				КОГДА TripsNonLawsonApproval.Status = ЗНАЧЕНИЕ(Перечисление.StatusesOfApproval.SentForApproval)
					ТОГДА "LEVEL_2_REQUIRED"
				КОГДА TripsNonLawsonApproval.Status = ЗНАЧЕНИЕ(Перечисление.StatusesOfApproval.Approved)
					ТОГДА "LEVEL_2_APPROVED"
				КОГДА TripsNonLawsonApproval.Status = ЗНАЧЕНИЕ(Перечисление.StatusesOfApproval.Rejected)
					ТОГДА "LEVEL_2_REJECTED"
				ИНАЧЕ "NOT_STARTED"
			КОНЕЦ
	КОНЕЦ,
	ВЫРАЗИТЬ(StagesOfTripsNonLawsonCompanies.Trip.MOT.FromTMSIDsList КАК СТРОКА(20)),
	ParcelsДетали.Ссылка.TransportRequest.PickUpWarehouse.RCACountry.TMSID,
	ParcelsДетали.Ссылка.TransportRequest.DeliverTo.RCACountry.TMSID,
	ParcelsДетали.Ссылка.TransportRequest.LegalEntity,
	ParcelsДетали.Ссылка.TransportRequest.LegalEntity.ERPID,
	ВЫБОР
		КОГДА ParcelsДетали.Ссылка.TransportRequest.LegalEntity.ERPID = "LAWSON"
			ТОГДА ParcelsДетали.Ссылка.TransportRequest.ActivityLawson
		ИНАЧЕ ParcelsДетали.Ссылка.TransportRequest.CostCenter.DefaultActivity
	КОНЕЦ,
	ВЫБОР
		КОГДА ParcelsДетали.Ссылка.TransportRequest.LegalEntity.CompanyCode &lt;&gt; ""
			ТОГДА ParcelsДетали.Ссылка.TransportRequest.LegalEntity.CompanyCode
		ИНАЧЕ ParcelsДетали.Ссылка.TransportRequest.LegalEntity.ParentCompany.CompanyNo
	КОНЕЦ,
	ВЫБОР
		КОГДА ГОД(TripNonLawsonCompaniesParcels.Ссылка.VerifiedByBillingSpecialist) = 1
			ТОГДА НЕОПРЕДЕЛЕНО
		ИНАЧЕ ГОД(TripNonLawsonCompaniesParcels.Ссылка.VerifiedByBillingSpecialist)
	КОНЕЦ,
	ВЫБОР
		КОГДА ГОД(TripNonLawsonCompaniesParcels.Ссылка.VerifiedByBillingSpecialist) = 1
			ТОГДА НЕОПРЕДЕЛЕНО
		ИНАЧЕ МЕСЯЦ(TripNonLawsonCompaniesParcels.Ссылка.VerifiedByBillingSpecialist)
	КОНЕЦ,
	ВЫБОР
		КОГДА ParcelsДетали.Ссылка.TransportRequest.LegalEntity.ERPID = "LAWSON"
			ТОГДА ВЫБОР
					КОГДА ParcelsДетали.Ссылка.TransportRequest.SegmentLawson.Код = "SDTR"
							ИЛИ ParcelsДетали.Ссылка.TransportRequest.SegmentLawson.Код = "BITS"
						ТОГДА "BDT"
					ИНАЧЕ ParcelsДетали.Ссылка.TransportRequest.SegmentLawson.Код
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ParcelsДетали.Ссылка.TransportRequest.CostCenter.Segment.Код = "SDTR"
						ИЛИ ParcelsДетали.Ссылка.TransportRequest.CostCenter.Segment.Код = "BITS"
					ТОГДА "BDT"
				ИНАЧЕ ParcelsДетали.Ссылка.TransportRequest.CostCenter.Segment.Код
			КОНЕЦ
	КОНЕЦ,
	TripNonLawsonCompaniesParcels.Ссылка.ModifiedBy.Код,
	StagesOfTripsNonLawsonCompanies.Trip.Номер,
	ParcelsДетали.Ссылка.TransportRequest.PickUpWarehouse.City,
	ParcelsДетали.Ссылка.TransportRequest.DeliverTo.City,
	TripNonLawsonCompaniesParcels.Ссылка.Дата,
	TripNonLawsonCompaniesParcels.Ссылка.Specialist,
	TripNonLawsonCompaniesParcels.Ссылка.VerifiedByBillingSpecialist,
	ParcelsДетали.Ссылка.CubicMeters / ParcelsДетали.Ссылка.NumOfParcels * ЕСТЬNULL(TripNonLawsonCompaniesParcels.NumOfParcels, ParcelsДетали.Ссылка.NumOfParcels)</query>
	</dataSet>
