		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ParcelsДетали.Ссылка.TransportRequest.Номер КАК ORDER_BASE_GID,
	"GENERAL CARGO DOMESTIC" КАК TR_TYPE,
	"ONLINE_TR_FORM" КАК CREATION_METHOD,
	"TR_RELEASED" КАК OB_RELEASE_STATUS,
	"NO" КАК IS_PARENT_TR,
	"NO" КАК IS_CHILD_TR,
	ParcelsДетали.Ссылка.Код КАК OB_SHIP_UNIT_GID,
	ВЫБОР
		КОГДА ParcelsДетали.Ссылка.TransportRequest.LegalEntity.ERPID = "LAWSON"
			ТОГДА ВЫБОР
					КОГДА ParcelsДетали.Ссылка.TransportRequest.SegmentLawson.Код = "SDTR"
							ИЛИ ParcelsДетали.Ссылка.TransportRequest.SegmentLawson.Код = "BITS"
						ТОГДА "BDT"
					ИНАЧЕ ParcelsДетали.Ссылка.TransportRequest.SegmentLawson.Код
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ParcelsДетали.Ссылка.TransportRequest.CostCenter.Segment.Код = "SDTR"
						ИЛИ ParcelsДетали.Ссылка.TransportRequest.CostCenter.Segment.Код = "BITS"
					ТОГДА "BDT"
				ИНАЧЕ ParcelsДетали.Ссылка.TransportRequest.CostCenter.Segment.Код
			КОНЕЦ
	КОНЕЦ КАК OB_SEGMENT,
	ParcelsДетали.Ссылка.TransportRequest.LegalEntity.Country.TMSID КАК PAYING_ENTITY_COUNTRY,
	ParcelsДетали.Ссылка.TransportRequest.LegalEntity.ERPID КАК PAYING_ERP_ID,
	ВЫБОР
		КОГДА ParcelsДетали.Ссылка.TransportRequest.LegalEntity.CompanyCode &lt;&gt; ""
			ТОГДА ParcelsДетали.Ссылка.TransportRequest.LegalEntity.CompanyCode
		ИНАЧЕ ParcelsДетали.Ссылка.TransportRequest.LegalEntity.ParentCompany.CompanyNo
	КОНЕЦ КАК PAYING_LEGAL_ENTITY,
	ParcelsДетали.Ссылка.TransportRequest.LegalEntity.Наименование КАК PAYING_LEGAL_ENTITY_NAME,
	ParcelsДетали.Ссылка.TransportRequest.CostCenter КАК PAYING_COST_CENTER,
	ВЫБОР
		КОГДА ParcelsДетали.Ссылка.TransportRequest.LegalEntity.ERPID = "LAWSON"
			ТОГДА ParcelsДетали.Ссылка.TransportRequest.ActivityLawson
		ИНАЧЕ ParcelsДетали.Ссылка.TransportRequest.CostCenter.DefaultActivity
	КОНЕЦ КАК PAYING_ACTIVITY_CODE,
	ВЫБОР
		КОГДА ParcelsДетали.Ссылка.HazardClass = ЗНАЧЕНИЕ(Справочник.HazardClasses.NonHazardous)
				ИЛИ ParcelsДетали.Ссылка.HazardClass = ЗНАЧЕНИЕ(Справочник.HazardClasses.ПустаяСсылка)
			ТОГДА "NO"
		ИНАЧЕ "YES"
	КОНЕЦ КАК CARGO_STATUS,
	ParcelsДетали.Ссылка.GrossWeightKG / ParcelsДетали.Ссылка.NumOfParcels * ЕСТЬNULL(TripNonLawsonCompaniesParcels.NumOfParcels, ParcelsДетали.Ссылка.NumOfParcels) КАК PACKAGE_WEIGHT,
	"LB" КАК PACKAGE_WEIGHT_UOM,
	ParcelsДетали.Ссылка.GrossWeightKG / ParcelsДетали.Ссылка.NumOfParcels * ЕСТЬNULL(TripNonLawsonCompaniesParcels.NumOfParcels, ParcelsДетали.Ссылка.NumOfParcels) / 0.45359 КАК WEIGHT_BASE,
	ParcelsДетали.Ссылка.CubicMeters / ParcelsДетали.Ссылка.NumOfParcels * ЕСТЬNULL(TripNonLawsonCompaniesParcels.NumOfParcels, ParcelsДетали.Ссылка.NumOfParcels) КАК PACKAGE_VOLUME,
	"CUMTR" КАК PACKAGE_VOLUME_UOM,
	ParcelsДетали.Ссылка.CubicMeters / ParcelsДетали.Ссылка.NumOfParcels * ЕСТЬNULL(TripNonLawsonCompaniesParcels.NumOfParcels, ParcelsДетали.Ссылка.NumOfParcels) КАК VOLUME_BASE,
	ParcelsДетали.Ссылка.LengthCM КАК LENGTH,
	"FT" КАК LENGTH_UOM_CODE,
	ParcelsДетали.Ссылка.WidthCM КАК WIDTH,
	"FT" КАК WIDTH_UOM_CODE,
	ParcelsДетали.Ссылка.HeightCM КАК HEIGHT,
	"FT" КАК HEIGHT_UOM_CODE,
	ParcelsДетали.Ссылка.TransportRequest.ReadyToShipLocalTime КАК EARLY_PICKUP_DATE,
	ParcelsДетали.Ссылка.TransportRequest.RequiredDeliveryLocalTime КАК REQUIRED_DELIVERY_DATE,
	TripNonLawsonCompaniesParcels.Ссылка.Specialist КАК ORIGIN_SLS,
	ParcelsДетали.Ссылка.TransportRequest.AcceptedBySpecialistLocalTime КАК ORIGIN_SLS_INSERT_DATE,
	ВЫБОР
		КОГДА ParcelsДетали.Ссылка.TransportRequest.Recharge
			ТОГДА ВЫБОР
					КОГДА ParcelsДетали.Ссылка.TransportRequest.RechargeType = ЗНАЧЕНИЕ(Перечисление.RechargeType.External)
						ТОГДА "E"
					ИНАЧЕ "I"
				КОНЕЦ
		ИНАЧЕ "N"
	КОНЕЦ КАК OB_RECHARGE_FLAG,
	ParcelsДетали.Ссылка.TransportRequest.PickUpWarehouse КАК OB_ORIGIN_LOCATION,
	ParcelsДетали.Ссылка.TransportRequest.PickUpWarehouse.City КАК ORIGIN_CITY,
	ParcelsДетали.Ссылка.TransportRequest.PickUpWarehouse.City КАК ORIGIN_PARENT_CITY,
	ParcelsДетали.Ссылка.TransportRequest.PickUpWarehouse.FacilityType КАК ORIGIN_FACILITY_TYPE,
	ParcelsДетали.Ссылка.TransportRequest.PickUpWarehouse.RegionProvince КАК ORIGIN_PROVINCE,
	ParcelsДетали.Ссылка.TransportRequest.PickUpWarehouse.RCACountry.TMSID КАК OB_ORIGIN_COUNTRY,
	ParcelsДетали.Ссылка.TransportRequest.PickUpWarehouse.CountryCode КАК OB_ORIGIN_CLC,
	ParcelsДетали.Ссылка.TransportRequest.DeliverTo КАК OB_DEST_LOCATION,
	ParcelsДетали.Ссылка.TransportRequest.DeliverTo.City КАК DESTINATION_CITY,
	ParcelsДетали.Ссылка.TransportRequest.DeliverTo.City КАК DESTINATION_PARENT_CITY,
	ParcelsДетали.Ссылка.TransportRequest.DeliverTo.FacilityType КАК DESTINATION_FACILITY_TYPE,
	ParcelsДетали.Ссылка.TransportRequest.DeliverTo.RegionProvince КАК DESTINATION_PROVINCE,
	ParcelsДетали.Ссылка.TransportRequest.DeliverTo.RCACountry.TMSID КАК OB_DEST_COUNTRY,
	ParcelsДетали.Ссылка.TransportRequest.DeliverTo.CountryCode КАК OB_DEST_CLC,
	ParcelsДетали.Ссылка.TransportRequest.Requestor КАК OB_REQUESTER,
	ParcelsДетали.Ссылка.TransportRequest.RequestedLocalTime КАК OB_INSERT_DATE,
	ГОД(TripNonLawsonCompaniesParcels.Ссылка.Дата) КАК OR_INSERT_YEAR,
	МЕСЯЦ(TripNonLawsonCompaniesParcels.Ссылка.Дата) КАК OR_INSERT_MONTH,
	МАКСИМУМ(ParcelsДетали.СтрокаИнвойса.ERPTreatmentNonLawson) КАК CARGO_TYPE,
	TripNonLawsonCompaniesParcels.Ссылка.Номер КАК ORDER_RELEASE_GID,
	ВЫБОР
		КОГДА StagesOfTransportRequests.Stage = ЗНАЧЕНИЕ(Перечисление.TransportRequestStages.Requested)
				ИЛИ StagesOfTransportRequests.Stage = ЗНАЧЕНИЕ(Перечисление.TransportRequestStages.AcceptedBySpecialist)
			ТОГДА "PLANNING_NEW"
		КОГДА StagesOfTransportRequests.Stage = ЗНАЧЕНИЕ(Перечисление.TransportRequestStages.PartiallyShipped)
			ТОГДА "PLANNING_PLANNED - PARTIAL"
