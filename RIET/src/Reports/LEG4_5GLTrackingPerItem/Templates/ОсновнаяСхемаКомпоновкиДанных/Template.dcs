		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	DOCs.Ссылка КАК DOC,
	DOCs.Номер КАК Номер,
	DOCs.Дата КАК Дата,
	DOCs.ProcessLevel.Corporation КАК Corporation,
	DOCs.Coordinator КАК Coordinator,
	DOCs.GOLD КАК GOLD,
	DOCs.Seller КАК Seller,
	DOCs.ConsignTo КАК ConsignTo,
	DOCs.POD КАК POD,
	DOCs.RequestedPOA КАК RequestedPOA,
	DOCs.MOT КАК MOT,
	DOCs.CCA КАК CCA,
	DOCs.Urgency КАК Urgency,
	DOCs.SegmentsList КАК SegmentsList,
	DOCs.InvoiceList КАК InvoiceList,
	DOCs.POList КАК POList,
	DOCs.CurrentComment КАК CurrentComment,
	DOCs.Requested КАК CCARequestedDate,
	DOCs.Received КАК CCAReceivedDate,
	DOCs.Granted КАК GLGrantedDate,
	DOCs.CurrentStage КАК CurrentStage,
	DOCs.CurrentStatus КАК CurrentStatus,
	DOCs.CurrentStatusDate КАК CurrentStatusDate,
	РАЗНОСТЬДАТ(DOCs.Дата, ВЫБОР
			КОГДА DOCs.Granted = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА &amp;Период
			ИНАЧЕ DOCs.Granted
		КОНЕЦ, ДЕНЬ) КАК SLBAgingDays,
	РАЗНОСТЬДАТ(DOCs.Дата, ВЫБОР
			КОГДА DOCs.Booked = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА &amp;Период
			ИНАЧЕ DOCs.Booked
		КОНЕЦ, ДЕНЬ) КАК TotalGLAgingDays,
	ВЫБОР
		КОГДА DOCs.Requested = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		ИНАЧЕ РАЗНОСТЬДАТ(DOCs.Requested, ВЫБОР
					КОГДА DOCs.Received = ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА &amp;Период
					ИНАЧЕ DOCs.Received
				КОНЕЦ, ДЕНЬ)
	КОНЕЦ КАК GLAgingDays,
	ВЫБОР
		КОГДА DOCs.CurrentStatus = ЗНАЧЕНИЕ(Перечисление.DOCStatuses.PendingCCA)
			ТОГДА РАЗНОСТЬДАТ(DOCs.CurrentStatusDate, &amp;Период, ДЕНЬ)
		ИНАЧЕ 0
	КОНЕЦ КАК PendingCCADays,
	ВЫБОР
		КОГДА DOCs.CurrentStatus = ЗНАЧЕНИЕ(Перечисление.DOCStatuses.PendingSLB)
			ТОГДА РАЗНОСТЬДАТ(DOCs.CurrentStatusDate, &amp;Период, ДЕНЬ)
		ИНАЧЕ 0
	КОНЕЦ КАК PendingSLBDays,
	ВЫБОР
		КОГДА DOCs.CurrentStatus = ЗНАЧЕНИЕ(Перечисление.DOCStatuses.PendingMOC)
			ТОГДА РАЗНОСТЬДАТ(DOCs.CurrentStatusDate, &amp;Период, ДЕНЬ)
		ИНАЧЕ 0
	КОНЕЦ КАК PendingMOCDays,
	ВЫБОР
		КОГДА DOCs.Received = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		ИНАЧЕ РАЗНОСТЬДАТ(DOCs.Received, ВЫБОР
					КОГДА DOCs.Granted = ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА &amp;Период
					ИНАЧЕ DOCs.Granted
				КОНЕЦ, ДЕНЬ)
	КОНЕЦ КАК ReceivedNotGrantedDays,
	ВЫБОР
		КОГДА DOCs.Granted = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		ИНАЧЕ РАЗНОСТЬДАТ(DOCs.Granted, ВЫБОР
					КОГДА DOCs.Booked = ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА &amp;Период
					ИНАЧЕ DOCs.Booked
				КОНЕЦ, ДЕНЬ)
	КОНЕЦ КАК GrantedNotShippedDays
ПОМЕСТИТЬ DOCs
ИЗ
	Документ.КонсолидированныйПакетЗаявокНаПеревозку КАК DOCs
ГДЕ
	НЕ DOCs.Отменен
	И НЕ DOCs.HouseKeeping
	И DOCs.ИмпортЭкспорт = ЗНАЧЕНИЕ(Перечисление.ИмпортЭкспорт.Import)
{ГДЕ
	DOCs.Ссылка.* КАК DOC,
	DOCs.ProcessLevel.Corporation.* КАК Corporation,
	DOCs.Coordinator.*,
	DOCs.GOLD,
	DOCs.Seller.*,
	DOCs.ConsignTo.*,
	DOCs.POD.*,
	DOCs.RequestedPOA.*,
	DOCs.MOT.*,
	DOCs.CCA.*,
	DOCs.Urgency.*,
	DOCs.CurrentStage.*,
	DOCs.CurrentStatus.*}

ИНДЕКСИРОВАТЬ ПО
	DOC
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	DOCs.DOC КАК DOC,
	DOCs.Номер,
	DOCs.Дата,
	DOCs.Corporation,
	DOCs.Coordinator,
	DOCs.GOLD,
	DOCs.Seller,
	DOCs.ConsignTo,
	DOCs.POD,
	DOCs.RequestedPOA,
	DOCs.MOT,
	DOCs.CCA,
	DOCs.Urgency,
	DOCs.SegmentsList,
	DOCs.InvoiceList,
	DOCs.POList,
	DOCs.CurrentComment,
	DOCs.CCARequestedDate,
	DOCs.CCAReceivedDate,
	DOCs.GLGrantedDate,
	DOCs.CurrentStage,
	DOCs.CurrentStatus,
	DOCs.CurrentStatusDate,
	DOCs.SLBAgingDays,
	DOCs.TotalGLAgingDays,
	DOCs.GLAgingDays,
	DOCs.PendingCCADays,
	DOCs.PendingSLBDays,
	DOCs.ReceivedNotGrantedDays,
	DOCs.GrantedNotShippedDays,
	СУММА(DOCsParcels.Parcel.NumOfParcels) КАК PiecesQty,
	СУММА(DOCsParcels.Parcel.GrossWeightKG) КАК GrossWeightKg,
	СУММА(DOCsParcels.Parcel.CubicMeters) КАК CubicMeters,
	"" КАК RootCauseOfDelayAndActionsTaken,
	DOCs.PendingMOCDays
ИЗ
	DOCs КАК DOCs
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.КонсолидированныйПакетЗаявокНаПеревозку.Parcels КАК DOCsParcels
		ПО DOCs.DOC = DOCsParcels.Ссылка

СГРУППИРОВАТЬ ПО
	DOCs.DOC,
	DOCs.Номер,
	DOCs.Дата,
	DOCs.GOLD,
	DOCs.POD,
	DOCs.CCA,
	DOCs.ConsignTo,
	DOCs.MOT,
	DOCs.RequestedPOA,
	DOCs.Urgency,
	DOCs.CCARequestedDate,
	DOCs.CCAReceivedDate,
	DOCs.GLGrantedDate,
	DOCs.CurrentStage,
	DOCs.CurrentStatus,
	DOCs.CurrentStatusDate,
	DOCs.Corporation,
	DOCs.Coordinator,
	DOCs.SLBAgingDays,
	DOCs.TotalGLAgingDays,
	DOCs.GLAgingDays,
	DOCs.PendingCCADays,
	DOCs.PendingSLBDays,
	DOCs.ReceivedNotGrantedDays,
	DOCs.GrantedNotShippedDays,
	DOCs.CurrentComment,
	DOCs.POList,
	DOCs.SegmentsList,
	DOCs.InvoiceList,
	DOCs.Seller,
	DOCs.PendingMOCDays</query>
	</dataSet>
