	ExportShipmentExportRequests.Ссылка.InternationalATD,
	ExportShipmentExportRequests.Ссылка.InternationalATA,
	ExportShipmentExportRequests.Ссылка.InternationalFreightProvider,
	СтрокиИнвойса.ExportRequest.ExportSpecialist КАК ExportSpecialist,
	СтрокиИнвойса.ExportRequest.Segment КАК Segment,
