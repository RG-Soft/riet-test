	ExportItems.InternationalATA,
	ExportItems.InternationalFreightProvider,
	ExportItems.ExportSpecialist,
	ExportItems.Segment,
	InvoiceLinesCostsОбороты.ДокументОснование,
	ExportItems.FromLegalEntity,
	ExportItems.AU,
