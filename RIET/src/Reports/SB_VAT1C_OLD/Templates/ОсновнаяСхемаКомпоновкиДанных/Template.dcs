		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЕСТЬNULL(ДоговорыКонтрагентовДолиУчастников.Контрагент.LawsonID, ВложенныйЗапрос.Регистратор.Контрагент.LawsonID) КАК РегистраторКонтрагентLawsonID,
	ЕСТЬNULL(ДоговорыКонтрагентовДолиУчастников.Контрагент, ВложенныйЗапрос.Регистратор.Контрагент) КАК РегистраторКонтрагент,
	ВложенныйЗапрос.Регистратор.НалоговыйПериод,
	ВложенныйЗапрос.Регистратор.Номер,
	ВложенныйЗапрос.Регистратор.Дата,
	ВложенныйЗапрос.Регистратор.ВалютаДокумента,
	ВложенныйЗапрос.Регистратор.Ответственный,
	ВложенныйЗапрос.Регистратор.ДатаПроведения,
	ВложенныйЗапрос.Регистратор,
	ВложенныйЗапрос1.Номер,
	ВложенныйЗапрос1.Дата,
	ВложенныйЗапрос.Регистратор.Контрагент.ИНН КАК РНН,
	ВложенныйЗапрос.Регистратор.Контрагент.КодПоОКПО КАК БИН,
	ВложенныйЗапрос.ДоговорКонтрагента,
	ДоговорыКонтрагентовДолиУчастников.Контрагент,
	ВЫБОР
		КОГДА ДоговорыКонтрагентовДолиУчастников.Доля ЕСТЬ NULL 
			ТОГДА 100
		ИНАЧЕ ДоговорыКонтрагентовДолиУчастников.Доля / СуммаДолейДоговора.СуммаДолей * 100
	КОНЕЦ КАК Доля,
	ДоговорыКонтрагентовДолиУчастников.Доля КАК Процент,
	СуммаДолейДоговора.СуммаДолей КАК СуммарныйПроцент
ПОМЕСТИТЬ Регистраторы
ИЗ
	(ВЫБРАТЬ РАЗЛИЧНЫЕ
		Продажи.Регистратор КАК Регистратор,
		Продажи.ДоговорКонтрагента КАК ДоговорКонтрагента
	ИЗ
		РегистрНакопления.Продажи КАК Продажи
	ГДЕ
		Продажи.Регистратор.ДатаПроведения МЕЖДУ &amp;Нач И &amp;Кон
		И Продажи.Регистратор.Проведен) КАК ВложенныйЗапрос
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ РАЗЛИЧНЫЕ
			СчетФактураВыданный.Номер КАК Номер,
			СчетФактураВыданный.Дата КАК Дата,
			СчетФактураВыданный.ДокументОснование КАК ДокументОснование
		ИЗ
			Документ.СчетФактураВыданный КАК СчетФактураВыданный
		ГДЕ
			СчетФактураВыданный.Проведен) КАК ВложенныйЗапрос1
		ПО ВложенныйЗапрос.Регистратор = ВложенныйЗапрос1.ДокументОснование
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ДоговорыКонтрагентов.ДолиУчастников КАК ДоговорыКонтрагентовДолиУчастников
		ПО ВложенныйЗапрос.ДоговорКонтрагента = ДоговорыКонтрагентовДолиУчастников.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ДоговорыКонтрагентовДолиУчастников.Ссылка КАК Ссылка,
			СУММА(ДоговорыКонтрагентовДолиУчастников.Доля) КАК СуммаДолей
		ИЗ
			Справочник.ДоговорыКонтрагентов.ДолиУчастников КАК ДоговорыКонтрагентовДолиУчастников
		
		СГРУППИРОВАТЬ ПО
			ДоговорыКонтрагентовДолиУчастников.Ссылка) КАК СуммаДолейДоговора
		ПО ВложенныйЗапрос.ДоговорКонтрагента = СуммаДолейДоговора.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Регистраторы.РегистраторНалоговыйПериод КАК TaxPeriod,
	Регистраторы.РегистраторНомер КАК Akt,
	Регистраторы.РегистраторДата КАК Akt_Date,
	Регистраторы.Номер КАК F_Invoice,
	Регистраторы.Дата КАК F_Invoice_Date,
	Регистраторы.РегистраторВалютаДокумента КАК Currency,
	ВЫБОР
		КОГДА Регистраторы.РегистраторВалютаДокумента.Код = "810"
			ТОГДА 0
		ИНАЧЕ ВложенныйЗапрос.Курс
	КОНЕЦ КАК ExchRate,
	ВложенныйЗапрос.СтавкаНДС КАК VatRate,
	СУММА(ВЫРАЗИТЬ(ВЫБОР
				КОГДА Регистраторы.РегистраторВалютаДокумента.Наименование = "USD"
					ТОГДА ВложенныйЗапрос.СуммаUSD - ВложенныйЗапрос.СуммаНДСUSD
				ИНАЧЕ ВложенныйЗапрос.СуммаRUR - ВложенныйЗапрос.СуммаНДСRUR
			КОНЕЦ * Регистраторы.Доля / 100 КАК ЧИСЛО(15, 2))) КАК D_Net,
	СУММА(ВЫРАЗИТЬ(ВЫБОР
				КОГДА Регистраторы.РегистраторВалютаДокумента.Наименование = "USD"
					ТОГДА ВложенныйЗапрос.СуммаНДСUSD
				ИНАЧЕ ВложенныйЗапрос.СуммаНДСRUR
			КОНЕЦ * Регистраторы.Доля / 100 КАК ЧИСЛО(15, 2))) КАК D_VAT,
	СУММА(ВЫРАЗИТЬ(ВЫБОР
				КОГДА Регистраторы.РегистраторВалютаДокумента.Наименование = "USD"
					ТОГДА ВложенныйЗапрос.СуммаUSD
				ИНАЧЕ ВложенныйЗапрос.СуммаRUR
			КОНЕЦ * Регистраторы.Доля / 100 КАК ЧИСЛО(15, 2))) КАК D_Gross,
	СУММА(ВЫРАЗИТЬ((ВложенныйЗапрос.СуммаRUR - ВложенныйЗапрос.СуммаНДСRUR) * Регистраторы.Доля / 100 КАК ЧИСЛО(15, 2))) КАК F_Net,
	СУММА(ВЫРАЗИТЬ(ВложенныйЗапрос.СуммаНДСRUR * Регистраторы.Доля / 100 КАК ЧИСЛО(15, 2))) КАК F_VAT,
	СУММА(ВЫРАЗИТЬ(ВложенныйЗапрос.СуммаRUR * Регистраторы.Доля / 100 КАК ЧИСЛО(15, 2))) КАК F_Gross,
	ВложенныйЗапрос.ПодразделениеОрганизации КАК Location,
	Регистраторы.РегистраторОтветственный КАК Operator,
	Регистраторы.РегистраторДатаПроведения КАК Posted,
	ЕСТЬNULL(Регистраторы.Контрагент.ИНН, Регистраторы.РНН) КАК TRN,
	ЕСТЬNULL(Регистраторы.Контрагент.КодПоОКПО, Регистраторы.БИН) КАК BIN,
	Регистраторы.Регистратор,
	ВЫРАЗИТЬ(Регистраторы.Доля КАК ЧИСЛО(5, 2)) КАК Доля,
	Регистраторы.РегистраторКонтрагентLawsonID КАК LawsonID,
	Регистраторы.РегистраторКонтрагент КАК ClientName,
	Регистраторы.Регистратор.Ответственный КАК User,
	ПРЕДСТАВЛЕНИЕ(Регистраторы.Процент) КАК Процент,
	ПРЕДСТАВЛЕНИЕ(Регистраторы.СуммарныйПроцент) КАК СуммарныйПроцент
ИЗ
	(ВЫБРАТЬ
		Продажи.Регистратор КАК Регистратор,
		Продажи.Курс КАК Курс,
		Продажи.СтавкаНДС КАК СтавкаНДС,
		Продажи.СуммаRUR КАК СуммаRUR,
		Продажи.СуммаUSD КАК СуммаUSD,
		Продажи.СуммаНДСRUR КАК СуммаНДСRUR,
		Продажи.СуммаНДСUSD КАК СуммаНДСUSD,
		Продажи.ПодразделениеОрганизации КАК ПодразделениеОрганизации
	ИЗ
		РегистрНакопления.Продажи КАК Продажи
	ГДЕ
		Продажи.Регистратор В
				(ВЫБРАТЬ
					Регистраторы.Регистратор
				ИЗ
					Регистраторы КАК Регистраторы)
		И Продажи.Тип &lt;&gt; "DIF") КАК ВложенныйЗапрос
		ЛЕВОЕ СОЕДИНЕНИЕ Регистраторы КАК Регистраторы
		ПО ВложенныйЗапрос.Регистратор = Регистраторы.Регистратор

СГРУППИРОВАТЬ ПО
	Регистраторы.РегистраторНалоговыйПериод,
	Регистраторы.РегистраторНомер,
	Регистраторы.РегистраторДата,
	Регистраторы.Номер,
	Регистраторы.Дата,
	Регистраторы.РегистраторВалютаДокумента,
	ВложенныйЗапрос.СтавкаНДС,
