	<calculatedField>
		<dataPath>AwaitingPermits</dataPath>
		<expression>ВЫБОР
			КОГДА CurrentStage = ПредопределенноеЗначение("Перечисление.ExportStages.AwaitingPermits")
					И НЕ PermitsRequested = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА 
				РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(PermitsRequested, "ДЕНЬ"), КОНЕЦПЕРИОДА(&amp;ТекущаяДата, "ДЕНЬ"))
			ИНАЧЕ ВЫБОР
					КОГДА PermitsRequested = ДАТАВРЕМЯ(1, 1, 1)
							ИЛИ PermitsObtained = ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА 0
					ИНАЧЕ 
					РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(PermitsRequested, "ДЕНЬ"), КОНЕЦПЕРИОДА(PermitsObtained, "ДЕНЬ"))
				КОНЕЦ
		КОНЕЦ * ВЫБОР
			КОГДА &amp;ЧасыДни = "Hours"
				ТОГДА 8
			ИНАЧЕ 1
		КОНЕЦ</expression>
