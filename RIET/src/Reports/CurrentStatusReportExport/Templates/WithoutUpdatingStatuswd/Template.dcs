				РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(LocalATA, "ДЕНЬ"), КОНЕЦПЕРИОДА(&amp;ТекущаяДата, "ДЕНЬ"))
			ИНАЧЕ ВЫБОР
					КОГДА LocalATA = ДАТАВРЕМЯ(1, 1, 1)
							ИЛИ PermitsRequested = ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА 0
					ИНАЧЕ 
					РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(LocalATA, "ДЕНЬ"), КОНЕЦПЕРИОДА(PermitsRequested, "ДЕНЬ"))
