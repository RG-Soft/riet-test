	<calculatedField>
		<dataPath>TotalWD</dataPath>
		<expression>ВЫБОР
            КОГДА естьNULL(InternationalATA, ДатаВремя(1,1,1)) = ДатаВремя(1,1,1) или 
  CreationDate= ДатаВремя(1,1,1)              ТОГДА РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НачалоПериода(CreationDate, "День"), КОНЕЦПЕРИОДА(&amp;ТекущаяДата, "ДЕНЬ"))
            ИНАЧЕ РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(InternationalATA, "ДЕНЬ"), КОНЕЦПЕРИОДА(CreationDate, "ДЕНЬ")) конец</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Total WD</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Total WD</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<calculatedField>
		<dataPath>Draft</dataPath>
		<expression>ВЫБОР
			КОГДА CurrentStage = ПредопределенноеЗначение("Перечисление.ExportStages.Draft")
				ТОГДА 
	 			РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(CreationDate, "ДЕНЬ"), КОНЕЦПЕРИОДА(&amp;ТекущаяДата, "ДЕНЬ"))
			ИНАЧЕ ВЫБОР
					КОГДА SubmittedDate = ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА 0
					ИНАЧЕ 
					РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(SubmittedDate, "ДЕНЬ"), КОНЕЦПЕРИОДА(CreationDate, "ДЕНЬ"))				
				КОНЕЦ
		КОНЕЦ * ВЫБОР
			КОГДА &amp;ЧасыДни = "Hours"
				ТОГДА 8
			ИНАЧЕ 1
		КОНЕЦ</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Draft</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Draft</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<calculatedField>
		<dataPath>Submitted</dataPath>
		<expression>ВЫБОР
			КОГДА CurrentStage = ПредопределенноеЗначение("Перечисление.ExportStages.Submitted")
					И НЕ  SubmittedDate = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА 
				РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(SubmittedDate, "ДЕНЬ"), КОНЕЦПЕРИОДА(&amp;ТекущаяДата, "ДЕНЬ"))
				
			ИНАЧЕ ВЫБОР
					КОГДА SubmittedDate = ДАТАВРЕМЯ(1, 1, 1)
							ИЛИ AcceptedBySpecialist = ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА 0
					ИНАЧЕ 
						РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(SubmittedDate, "ДЕНЬ"), КОНЕЦПЕРИОДА(AcceptedBySpecialist, "ДЕНЬ"))
				 
				КОНЕЦ
		КОНЕЦ * ВЫБОР
			КОГДА &amp;ЧасыДни = "Hours"
				ТОГДА 8
			ИНАЧЕ 1
		КОНЕЦ</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Submitted</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Submitted</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<calculatedField>
		<dataPath>AcceptedBySpecialist</dataPath>
		<expression>ВЫБОР
			КОГДА CurrentStage = ПредопределенноеЗначение("Перечисление.ExportStages.AcceptedBySpecialist")
					И НЕ AcceptedBySpecialist = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА 
				РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(AcceptedBySpecialist, "ДЕНЬ"), КОНЕЦПЕРИОДА(&amp;ТекущаяДата, "ДЕНЬ"))
				
			ИНАЧЕ ВЫБОР
					КОГДА AcceptedBySpecialist = ДАТАВРЕМЯ(1, 1, 1)
							ИЛИ CCAGLRequested = ДАТАВРЕМЯ(1, 1, 1)
						ТОГДА 0
					ИНАЧЕ 
					РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(AcceptedBySpecialist, "ДЕНЬ"), КОНЕЦПЕРИОДА(CCAGLRequested, "ДЕНЬ"))
				
				КОНЕЦ
		КОНЕЦ * ВЫБОР
			КОГДА &amp;ЧасыДни = "Hours"
				ТОГДА 8
			ИНАЧЕ 1
		КОНЕЦ</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Accepted by specialist</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Accepted by specialist</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<calculatedField>
		<dataPath>GLAndQuotes</dataPath>
		<expression>ВЫБОР
			КОГДА CurrentStage = ПредопределенноеЗначение("Перечисление.ExportStages.GLAndQuotes")
					И НЕ CCAGLRequested = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА 
				РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(CCAGLRequested, "ДЕНЬ"), КОНЕЦПЕРИОДА(&amp;ТекущаяДата, "ДЕНЬ"))		
			ИНАЧЕ ВЫБОР
					КОГДА (CCAGLReceived = ДАТАВРЕМЯ(1, 1, 1)
							ИЛИ CCAGLRequested = ДАТАВРЕМЯ(1, 1, 1))
							И (ConsigneeGLRequested = ДАТАВРЕМЯ(1, 1, 1)
								ИЛИ ConsigneeGLReceived = ДАТАВРЕМЯ(1, 1, 1))
						ТОГДА 0
					ИНАЧЕ ВЫБОР
							КОГДА ConsigneeGLReceived &gt; CCAGLReceived
								ТОГДА 
								РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(ConsigneeGLRequested, "ДЕНЬ"), КОНЕЦПЕРИОДА(ConsigneeGLReceived, "ДЕНЬ"))								
							ИНАЧЕ 
							РГСофт.РасчитатьКоличествоРабочихДнейПериодаПоКалендарю(Истина,НАЧАЛОПЕРИОДА(CCAGLRequested, "ДЕНЬ"), КОНЕЦПЕРИОДА(CCAGLReceived, "ДЕНЬ"))
						КОНЕЦ
				КОНЕЦ
		КОНЕЦ * ВЫБОР
			КОГДА &amp;ЧасыДни = "Hours"
				ТОГДА 8
			ИНАЧЕ 1
		КОНЕЦ</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>GL and quotes</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>GL and quotes</v8:content>
			</v8:item>
		</title>
	</calculatedField>
