	СтруктураНастройки.Вставить("Имя", "ОткрытиеИзBatchOfCustomsFiles");
	СтруктураНастройки.Вставить("BatchOfCustomsFiles", ПараметрКоманды);
	
	ПараметрыФормы = Новый Структура;
