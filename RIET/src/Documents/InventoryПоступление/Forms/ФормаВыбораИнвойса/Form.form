    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА ВложенныйЗапрос1.СтрокаИнвойсаЕдиницаИзмерения = &amp;UOM
			ТОГДА ВложенныйЗапрос1.Сумма
		ИНАЧЕ ВложенныйЗапрос1.Сумма * &amp;ConversionFactor
	КОНЕЦ КАК Цена,
	ВложенныйЗапрос1.Инвойс,
	ВЫБОР
		КОГДА ВложенныйЗапрос1.СтрокаИнвойсаЕдиницаИзмерения = &amp;UOM
			ТОГДА ВложенныйЗапрос1.Количество
		ИНАЧЕ ВложенныйЗапрос1.Количество / &amp;ConversionFactor
	КОНЕЦ КАК Количество,
	ВложенныйЗапрос1.Признак,
	ВложенныйЗапрос1.НаименованиеТовара,
	ВложенныйЗапрос1.Код,
	ВложенныйЗапрос1.СуммаОбщая КАК Сумма,
