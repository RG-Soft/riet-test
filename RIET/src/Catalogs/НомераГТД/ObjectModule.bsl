
Процедура ПередЗаписью(Отказ)
	запрос = новый запрос;
	запрос.текст ="ВЫБРАТЬ
	              |	НомераГТД.Ссылка
	              |ИЗ
	              |	Справочник.НомераГТД КАК НомераГТД
	              |ГДЕ
	              |	НомераГТД.Код ПОДОБНО &Код
	              |	И НомераГТД.Ссылка <> &ссылка";
	кк = сокрлп(код);
				   
	Запрос.УстановитьПараметр("Код", "%" + лев(кк,16)+ "_" + прав(кк, 6)+ "%");
	Запрос.УстановитьПараметр("ссылка", Ссылка);
	тз = запрос.выполнить().Выгрузить();
	
	Если тз.Количество()>0 тогда
		сообщить("Уже существует ГТД " + тз[0].ссылка);
		отказ = истина
	КонецЕсли;
КонецПроцедуры
