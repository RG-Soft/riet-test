	ОбщегоНазначения.ДобавитьПрефиксУзла(Префикс);
	Если  Не ЭтоГруппа Тогда
		Если ОбщегоНазначения.ЗначениеНеЗаполнено(ВидРБП) Тогда
			ВидРБП = Перечисления.ВидыРБП.Прочие;
		КонецЕсли;
		Если ОбщегоНазначения.ЗначениеНеЗаполнено(СпособПризнанияРасходов) Тогда
			СпособПризнанияРасходов = Перечисления.СпособыПризнанияРасходов.ПоМесяцам;
		КонецЕсли;
		
	КонецЕсли;
	
